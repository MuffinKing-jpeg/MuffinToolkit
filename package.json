{
  "name": "muffin-toolkit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --localize",
    "build-dev": "ng build --configuration development --localize",
    "watch": "ng build --watch --configuration development --localize",
    "test": "ng test",
    "translate-uk": "xlf2xlf -i src/locale/source/messages.xlf -o src/locale/translated/uk/messages.xlf -f en -t uk -c 1",
    "translate-ru": "xlf2xlf -i src/locale/source/messages.xlf -o src/locale/translated/ru/messages.xlf -f en -t ru -c 1",
    "extract-locale": "ng extract-i18n --output-path src/locale/source",
    "translate": "npm run extract-locale && npm run translate-ru && npm run translate-uk",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.1.3",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@faker-js/faker": "^8.0.2",
    "ngx-scrollbar": "^13.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlf2xlf": "^1.1.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/localize": "^16.0.3",
    "@mertasan/tailwindcss-variables": "^2.6.1",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.27.5",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-tailwind": "^4.0.0",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.2"
  }
}
